{
    "version": "3.24.238.0",
    "description": "Windows clipboard extension.",
    "homepage": "http://ditto-cp.sourceforge.net/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://sourceforge.net/projects/ditto-cp/files/Ditto/3.24.238.0/DittoSetup_64bit_3_24_238_0.exe",
            "hash": "24fcb7577a7c4e585c886fd93604f3da8d7cce032a8423cb45ce83e5ed89af80"
        },
        "32bit": {
            "url": "https://sourceforge.net/projects/ditto-cp/files/Ditto/3.24.238.0/DittoSetup_3_24_238_0.exe",
            "hash": "c82b2e22e3d82dc1330c4c980544eac6ed50b7bc5d913275257286b631c97d61"
        }
    },
    "extract_dir": "Ditto",
    "installer": {
        "args": [
            "/VERYSILENT",
            "/DIR=\"$dir\"",
            "/NORESTART"
        ]
    },
    "post_install": "Remove-Item -Recurse \"$([Environment]::GetFolderPath('commonstartmenu'))\\Programs\\Ditto\"",
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/VERYSILENT",
            "/NORESTART"
        ]
    },
    "bin": "Ditto.exe",
    "shortcuts": [
        [
            "Ditto.exe",
            "Ditto"
        ]
    ],
    "checkver": "var versionDots=\"([\\d.]+)\"",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://sourceforge.net/projects/ditto-cp/files/Ditto/$version/DittoSetup_64bit_$underscoreVersion.exe"
            },
            "32bit": {
                "url": "https://sourceforge.net/projects/ditto-cp/files/Ditto/$version/DittoSetup_$underscoreVersion.exe"
            }
        }
    }
}
