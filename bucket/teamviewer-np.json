    "installer": {
        "script": [
            "Invoke-ExternalCommand \"$dir\\setup.exe\" -ArgumentList @('/S', '/norestart') -RunAs | Out-Null",
            "Remove-Item \"$([Environment]::GetFolderPath('commonstartmenu'))\\Programs\\TeamViewer.lnk\"",
            "Remove-Item \"$dir\\setup.exe\""
