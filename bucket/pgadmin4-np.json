{
    "version": "6.3",
    "homepage": "https://www.pgadmin.org/",
    "description": "PostgreSQL administration and development platform.",
    "license": "PostgreSQL",
    "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v6.3/windows/pgadmin4-6.3-x64.exe",
    "hash": "a7a95d03d473f443e706329df4ca950d4a7afb1ce1cb6faee5da867fbc5a4eaa",
    "installer": {
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart",
            "/dir=$dir"
        ]
    },
    "post_install": [
        "$folder = \"$([Environment]::GetFolderPath('commonstartmenu'))\\Programs\\pgAdmin 4\"; if (Test-Path -Path $folder) { Remove-Item -Recurse $folder }",
        "$folder = \"$([Environment]::GetFolderPath('startmenu'))\\Programs\\pgAdmin 4\"; if (Test-Path -Path $folder) { Remove-Item -Recurse $folder }"
    ],
    "uninstaller": {
        "file": "unins000.exe",
        "args": [
            "/verysilent",
            "/nocancel",
            "/norestart"
        ]
    },
    "bin": [
        "runtime\\psql.exe",
        "runtime\\pgAdmin4.exe",
        "runtime\\pg_restore.exe",
        "runtime\\pg_dumpall.exe",
        "runtime\\pg_dump.exe"
    ],
    "shortcuts": [
        [
            "runtime\\pgAdmin4.exe",
            "pgAdmin 4"
        ]
    ],
    "checkver": {
        "url": "https://www.pgadmin.org/download/pgadmin-4-windows/",
        "regex": "pgAdmin 4 v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://ftp.postgresql.org/pub/pgadmin/pgadmin4/v$version/windows/pgadmin4-$version-x64.exe"
    }
}
